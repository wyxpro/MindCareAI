# 应用配置
NODE_ENV=development
PORT=3001
API_PREFIX=api/v1

# 数据库连接 (PostgreSQL)
# 请使用本地数据库或 Supabase 提供的连接字符串
DATABASE_URL=postgresql://postgres:[password]@localhost:5432/mindcareai

# JWT 配置
# 生成密钥: openssl rand -base64 32
JWT_SECRET=your_jwt_secret_here_change_this_in_production

# StepFun AI / Gateway 配置
# 获取 API Key: https://platform.stepfun.com/
INTEGRATIONS_API_KEY=your_stepfun_api_key_here
STEPFUN_API_URL=https://api.stepfun.com/v1/chat/completions
MULTIMODAL_API_URL=https://api.stepfun.com/v1/chat/completions

# AI 开关 (true=使用真实AI, false=使用模拟数据)
AI_USE_REAL=false

# CORS 配置
CORS_ENABLED=true
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
